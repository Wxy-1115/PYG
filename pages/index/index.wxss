.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  width: 100%;
  display: flex;
}
.index_cate navigator {
  padding: 20rpx;
  flex: 1;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floor_title image {
  width: 100%;
}
.index_floor .floor_list {
  overflow: hidden;
}
.index_floor .floor_list navigator {
  width: 33.33%;
  float: left;
}
.index_floor .floor_list navigator image {
  width: 100%;
}
.index_floor .floor_list navigator:nth-child(n+2) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_list navigator:nth-child(n+2):nth-child(2),
.index_floor .floor_list navigator:nth-child(n+2):nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_list navigator:nth-child(n+2) image {
  width: 100%;
  height: 100%;
}
